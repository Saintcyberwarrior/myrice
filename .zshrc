    prompt_symbol=ã‰¿
    [ "$EUID" -eq 0 ] && prompt_symbol=ðŸ’€
    case "$PROMPT_ALTERNATIVE" in
        twoline)
            PROMPT=$'%F{%(#.yellow.green)}(%B%F{%(#.red.yellow)}%n$%m%b%F{%(#.yellow.green)})-[%B%F{reset}%(6~.%-1~/â€¦/%4~.%5~)%b%F{%(#.yellow.green)}]\nâ””â”€%B%(#.%F{red}#.%F{yellow}$)%b%F{reset} '
            RPROMPT=$'%(?.. %? %F{red}%Bâ¨¯%b%F{reset})%(1j. %j %F{yellow}%Bâš™%b%F{reset}.)'
            ;;
        oneline)
